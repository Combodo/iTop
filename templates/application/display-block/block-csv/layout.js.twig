{% apply spaceless %}
$.post(
    '{{ oUIBlock.sAjaxLink|escape('js') }}',
    {{ oUIBlock.sJsonParams|raw }},
    function(data) {
        $('#csv_content').html(data);
        $('#csv_content_loading').hide();
        $('#csv_content').show();
    });
{% endapply %}