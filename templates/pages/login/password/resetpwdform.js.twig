function DoCheckPwd()
{
    if ($('#new_pwd').val() != $('#retype_new_pwd').val())
    {
        alert('{{ 'UI:Login:RetypePwdDoesNotMatch'|dict_s }}');
        return false;
    }
    return true;
}
