        <field id="key_words" xsi:type="AttributeString">
          <sql>key_words</sql>
          <default_value/>
          <is_null_allowed>true</is_null_allowed>
        </field>
        <field id="domains" xsi:type="AttributeTagSet">
          <sql>domains</sql>
          <is_null_allowed>true</is_null_allowed>
