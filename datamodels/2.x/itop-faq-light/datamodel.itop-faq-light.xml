          <active>true</active>
          <rank>
            <default>40</default>
          </rank>
          <width>12</width>
          <title>
            <navigation_menu>Brick:Portal:FAQ:Menu</navigation_menu>
            <home>Brick:Portal:FAQ:Title</home>
          </title>
          <description>Brick:Portal:FAQ:Title+</description>
          <decoration_class>
            <default>fc fc-life-saver fc-2x</default>
          </decoration_class>
          <levels>
