$("#do-update").on("click", function(oEvent) {
    $(this).prop("disabled", true);
    $("#cancel-update").prop("disabled", true);
    $(this).parents('form').submit();
    oEvent.preventDefault();
    oEvent.stopPropagation();
    return false;
});
